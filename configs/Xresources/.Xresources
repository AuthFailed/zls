! ------------------------------------------------------------------------------
! rofi mrczlnks color theme
! ------------------------------------------------------------------------------
rofi.location:          0 
rofi.color-enabled:     true
!                       'background', 'border'
rofi.color-window:      argb:dc121212, argb:dc333333
! State:                'bg',          'fg',   'bgalt',       'hlbg',        'hlfg'
rofi.color-normal:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ff00ff00
rofi.color-urgent:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ff00ff00
rofi.color-active:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ff00ff00
rofi.separator-style:   dash
rofi.fullscreen:        false
rofi.padding:           32
rofi.hide-scrollbar:    false
rofi.line-margin:       3
rofi.fake-transparency: false
rofi.yoffset:           0
rofi.xoffset:           0
rofi.width:             854
rofi.opacity:           30
rofi.font:              Fira Code Regular 14
